; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Vision Tower Game"
run/main_scene="res://scenes/splash.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://icon.svg"

[autoload]

SceneManager="*res://globals/scene_manager.gd"
Utilties="*res://globals/utilties.gd"

[display]

window/size/viewport_width=768
window/size/viewport_height=1280
window/emulate_mouse_from_touch=false

[global_group]

player=""
enemies=""
projectiles=""
pylons=""

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="Enemy"
2d_physics/layer_3="Projectile"
2d_physics/layer_4="Pylon"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
